(function() {
    'use strict';

    angular
        .module("kayApp")
        .config(stateConfig);

    stateConfig.$inject = ['$stateProvider'];

    function stateConfig($stateProvider) {

        $stateProvider.state({
            name: 'main',
            url: '/main',
            templateUrl: 'app/layouts/main/main.html'
        });
    }

})();