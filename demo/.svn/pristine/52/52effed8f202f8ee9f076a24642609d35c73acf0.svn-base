}
.text-right {
  text-align: right!important;
}
/*form*/
label {
  font-weight: 400;
}
.form.form-horizontal .form-group {
  margin: 0;
}
.form.form-horizontal .form-group .group-btn,
.form.form-horizontal .form-group .group-btn > .form-control {
  display: inline-block;
}
.form.form-horizontal .form-group .form-control {
  width: 200px;
  height: 30px;
  margin-right: 20px;
  padding: 2px 12px;
}
.form.form-horizontal .group-wrap {
  width: auto;
  display: inline-block;
  padding: 5px 5px 5px 15px;
  border-radius: 2px;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -ms-border-radius: 2px;
  -o-border-radius: 2px;
}
.form.form-horizontal .group-wrap .form-control,
.form.form-horizontal .group-wrap .btn {
  height: 28px;
}
.form.form-horizontal .group-wrap .form-control {
  border: 0;
  width: 300px;
}
.form.form-horizontal .group-wrap label {
  color: #fff;
}
.form.form-horizontal .group-wrap.cyan {
  background-color: #598EEE;
}
.form.form-horizontal.border-bottom {
  border-bottom: solid 1px #ddd;
  padding-bottom: 15px;
  margin-bottom: 15px;
}
.form-horizontal .form-group .control-label {
  padding-right: 0;
}
.form-horizontal .form-group .input-group .form-control {
  margin-right: 0;
}
.form-horizontal .form-group .input-group .input-group-addon {
  border-right: 0;
  border-left: 0;
}
/*table*/
.table-scrollable {
  margin-top: 10px;
  width: 100%;
  overflow: auto;
}
.table-scrollable .table thead > tr > th {
  background-color: #E3E4E9;
  color: #9e9e9e;
}
.table-scrollable .table thead > tr > th,
.table-scrollable .table tbody > tr > td {
  text-align: center;
  border: 1px solid #c3c3c3!important;
  vertical-align: middle;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: break-all;
  max-width: 250px;
}
.table-scrollable .table tbody > tr > td > a {
  margin-right: 10px;
}
.table-scrollable .table tbody > tr > td > a:last-child {
  margin-right: 0;
}
.table-scrollable .table tbody > tr > td .form-control {
  height: 28px;
  padding: 0 12px;
}
/*modal*/
.modal-header {
  padding: 10px 15px;
  background-color: #4A7DD9;
  color: #fff;
}
.modal-header .close {
  color: #fff!important;
  opacity: 1;
}
.modal-header .modal-title {
  font-size: 14px;
}
.modal-footer {
  padding: 10px 15px;
}
/*loading*/
.loading-warp {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.45);
  z-index: 999;
}
.loading-warp > .lading-img {
  background-image: url("../images/loading.gif");
  background-size: 50px 50px;
  width: 50px;
  height: 50px;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
/*paging*/
.paging-wrap > li {
  float: left;
  margin-right: 5px;
}
.paging-wrap > li .btn {
  padding: 4px 6px;
}
.paging-wrap > li > span {
  margin-top: 4px;
  display: inline-block;
}
.paging-wrap > li .form-control {
  height: 30px;
}
.paging-wrap > li:last-child {
  margin-right: 0;
}
/*popover*/
.pos-popover {
  position: relative;
  display: inline-block;
}
.pos-popover .popover-wrap {
  position: absolute;
  background-color: #fff;
  border: solid 1px #ddd;
  padding: 15px;
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -webkit-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -ms-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -o-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  max-height: 390px;
  z-index: 99;
  display: none;
}
.pos-popover .popover-wrap:before,
.pos-popover .popover-wrap:after {
  content: "";
  position: absolute;
  border-right: 10px solid transparent;
  border-left: 10px solid transparent;
}
.pos-popover .popover-wrap.bottom-right:before,
.pos-popover .popover-wrap.bottom-left:before {
  border-bottom: 14px solid #b9b9b9;
  top: -14px;
}
.pos-popover .popover-wrap.bottom-right:after,
.pos-popover .popover-wrap.bottom-left:after {
  border-bottom: 14px solid #fff;
  top: -13px;
}
.pos-popover .popover-wrap.top-right:before,
.pos-popover .popover-wrap.top-left:before {
  border-top: 14px solid #b9b9b9;
  bottom: -14px;
}
.pos-popover .popover-wrap.top-right:after,
.pos-popover .popover-wrap.top-left:after {
  border-top: 14px solid #fff;
  bottom: -13px;
}
.pos-popover .popover-wrap.bottom-right {
  top: 30px;
  left: 0;
}
.pos-popover .popover-wrap.bottom-right:before,
.pos-popover .popover-wrap.bottom-right:after {
  left: 15px;
}
.pos-popover .popover-wrap.bottom-left {
  top: 30px;
  right: 0;
}
.pos-popover .popover-wrap.bottom-left:before,
.pos-popover .popover-wrap.bottom-left:after {
  right: 15px;
}
.pos-popover .popover-wrap.top-right {
  bottom: 30px;
  left: 0;
}
.pos-popover .popover-wrap.top-right:before,
.pos-popover .popover-wrap.top-right:after {
  left: 15px;
}
.pos-popover .popover-wrap.top-left {
  bottom: 30px;
  right: 0;
}
.pos-popover .popover-wrap.top-left:before,
.pos-popover .popover-wrap.top-left:after {
  right: 15px;
}
.pos-popover .popover-wrap.list-group {
  padding: 0;
  border: 0;
}
.pos-popover .popover-wrap.list-group .list-group-item {
  border-radius: 0;
}
.pos-popover .popover-wrap.list-group .list-group-item:hover,
.pos-popover .popover-wrap.list-group .list-group-item.active {
  font-weight: bold;
}
.pos-popover:hover .popover-wrap {
  display: inline-block;
}
/*时间控件*/
.input-group {
  /*按钮组*/
}
.input-group .input-icon.right {
