/*navbar*/
.pages-top-navbar {
  width: 100%;
  height: 60px;
  background-color: #5A8EEE;
  position: fixed;
  padding-left: 200px;
  box-shadow: 0 2px 4px 0 #cccccc;
  -webkit-box-shadow: 0 2px 4px 0 #cccccc;
  -moz-box-shadow: 0 2px 4px 0 #cccccc;
  -ms-box-shadow: 0 2px 4px 0 #cccccc;
  -o-box-shadow: 0 2px 4px 0 #cccccc;
  z-index: 10;
}
.pages-top-navbar .navbar-left-list {
  width: 100%;
  height: 100%;
  padding-right: 450px;
  clear: both;
}
.pages-top-navbar .navbar-left-list > li {
  height: 60px;
  line-height: 60px;
  border-right: solid 1px #74a3f9;
  width: 230px;
  float: left;
  text-align: center;
  font-weight: bold;
  color: #fff;
}
.pages-top-navbar .navbar-left-list > li:hover,
.pages-top-navbar .navbar-left-list > li.active {
  background-color: #3B71D4;
}
.pages-top-navbar .navbar-left-list > li:hover {
  cursor: pointer;
}
.pages-top-navbar .navbar-left-list > li.active {
  position: relative;
}
.pages-top-navbar .navbar-left-list > li.active:after,
.pages-top-navbar .navbar-left-list > li.active:before {
  content: "";
  position: absolute;
  z-index: 99;
}
.pages-top-navbar .navbar-left-list > li.active:after {
  border-right: 9px solid transparent;
  border-left: 9px solid transparent;
  border-bottom: 13px solid #fff;
  top: 68px;
  left: 50.5%;
}
