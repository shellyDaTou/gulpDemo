/*color*/
body.body-bg {
  background-color: #E5E5E5;
}
.container-content,
.page-container.homepage .page-content {
  width: 1300px;
  margin: 0 auto;
  clear: both;
  display: table;
}
/*homepage*/
.has-header-action .page-container.homepage .page-content-wrapper {
  padding-top: 111px;
}
.page-header {
  padding: 0;
  margin: 0;
  border-bottom: 0;
}
.page-header .navbar {
  margin-bottom: 0;
}
.page-header .navbar-theme {
  background-color: #598EEE;
  border-color: #4A7DD9;
}
.page-header .navbar-nav > li > a {
  color: #fff;
}
.page-header .navbar-nav > li > a:hover,
.page-header .navbar-nav > li > a:visited {
  background-color: #598EEE;
}
.page-header .navbar-nav > li > a:focus {
  background-color: #598EEE;
}
.page-header .navbar-header .navbar-brand {
  padding: 0;
}
.page-header .navbar-nav.navbar-left > li > a {
  font-size: 16px;
}
.page-header .navbar-nav.navbar-left > li.active > a {
  background-color: #4A7DD9;
}
.page-header .navbar-nav.navbar-right {
  margin-right: 0;
}
.page-header .navbar-nav.navbar-right > li.user-info {
  position: relative;
  cursor: pointer;
}
.page-header .navbar-nav.navbar-right > li.user-info > a {
  padding: 0 20px;
  margin: 15px 0;
  position: relative;
}
.page-header .navbar-nav.navbar-right > li.user-info > a:after {
  position: absolute;
  content: '';
  bottom: 0;
  right: 0;
  width: 0;
  height: 0;
  border: 4px solid #fff;
  border-left-color: transparent;
  border-top-color: transparent;
}
.page-header .navbar-nav.navbar-right > li.user-info .user-info-action {
  position: absolute;
  right: 0;
  top: 101%;
  z-index: 2;
  border: 1px solid #ddd;
  border-radius: 5px;
  background-color: #fff;
  border-bottom: 2px solid #598EEE;
}
.page-header .navbar-nav.navbar-right > li.user-info .user-info-action:before,
.page-header .navbar-nav.navbar-right > li.user-info .user-info-action:after {
  content: '';
  position: absolute;
  right: 5px;
  top: -16px;
  width: 0;
  height: 0;
  z-index: 3;
  border-width: 8px;
  border-style: solid;
  border-color: transparent transparent #ddd transparent;
}
.page-header .navbar-nav.navbar-right > li.user-info .user-info-action:after {
  top: -15px;
  border-color: transparent transparent #fff transparent;
}
.page-header .navbar-nav.navbar-right > li.user-info .user-info-action .info-action-list {
  padding: 5px 15px;
}
.page-container {
  width: 100%;
  height: 100%;
  background-color: #E5E5E5;
}
.page-footer {
  width: 100%;
  font-size: 12px;
  color: #8b8b8b;
  margin: 5px 0;
  text-align: center;
}
.page-header-action {
  background-color: #fff;
  width: 100%;
  position: fixed;
  z-index: 10;
  top: 51px;
  padding: 10px 0;
  -webkit-box-shadow: 2px 2px 6px #bfbfbf;
  -moz-box-shadow: 2px 2px 6px #bfbfbf;
  box-shadow: 2px 2px 6px #bfbfbf;
}
.page-header-action .header-action-left {
  float: left;
}
.page-header-action .header-action-left .has-feedback .form-control {
  height: 30px;
}
.page-header-action .header-action-left .action-left-title {
  display: inline-block;
  font-size: 16px;
  margin-left: 10px;
  line-height: 26px;
}
.page-header-action .header-action-right {
  float: right;
}
.page-header-action .header-action-right .all-customer-total {
  padding-right: 20px;
}
.page-header-action .header-action-right .all-customer-total > span {
  margin-right: 20px;
}
.imitation-table-group .imitation-table-list.mark-danger-icon > .icon:before,
.imitation-table-group .imitation-table-list.mark-warning-icon > .icon:before {
  display: block;
  width: 19px;
  height: 19px;
  content: '';
  position: absolute;
  right: -5px;
  top: -5px;
  background: url("../images/imitation_table_icon.png") no-repeat;
}
.imitation-table-group .imitation-table-list.mark-danger-icon > .icon:before {
  background-position: -7px -110px;
}
.imitation-table-group .imitation-table-list.mark-warning-icon > .icon:before {
  background-position: -7px -144px;
}
/*imitation-table*/
.imitation-table-wrap {
  display: table;
  width: 100%;
}
.imitation-table-wrap .imitation-table-group {
  width: 100%;
  max-width: 100%;
  background-color: #fff;
  margin-bottom: 10px;
  padding: 15px;
  border: 1px solid #ddd;
  display: table;
}
.imitation-table-wrap .imitation-table-group:hover {
  border: 2px solid #598EEE;
  -webkit-box-shadow: 2px 1px 8px #bfbfbf;
  -moz-box-shadow: 2px 1px 8px #bfbfbf;
  box-shadow: 2px 1px 8px #bfbfbf;
}
.imitation-table-wrap .imitation-table-group:after {
  content: '';
  line-height: 1;
}
.imitation-table-wrap .imitation-table-group.first-child {
  margin-top: 10px;
}
.imitation-table-wrap .imitation-table-group .imitation-table-list {
  float: left;
  vertical-align: middle;
  min-height: 1px;
  padding: 10px;
}
.imitation-table-wrap .imitation-table-group .imitation-table-list.fixed-width {
  width: 330px;
}
.imitation-table-wrap .imitation-table-group .imitation-table-list.fixed-width-250 {
  width: 250px;
}
.imitation-table-wrap .imitation-table-group .imitation-table-list.fixed-width-150 {
  width: 120px;
}
.imitation-table-wrap .imitation-table-group.table-button-right {
  position: relative;
  padding-right: 245px;
}
.imitation-table-wrap .imitation-table-group.table-button-right .table-button-group {
  display: none;
}
.imitation-table-wrap .imitation-table-group.table-button-right:hover .table-button-group,
.imitation-table-wrap .imitation-table-group.table-button-right:hover .tips {
  display: block;
}
.imitation-table-wrap .imitation-table-group.test-environment {
  position: relative;
}
.imitation-table-wrap .imitation-table-group.test-environment:before,
.imitation-table-wrap .imitation-table-group.test-environment:after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 0;
  height: 0;
}
.imitation-table-wrap .imitation-table-group.test-environment:before {
  border: 18px solid #598EEE;
  border-right-color: transparent;
  border-bottom-color: transparent;
}
.imitation-table-wrap .imitation-table-group.test-environment:after {
  content: 'æµ‹';
  color: #fff;
  top: 3px;
  left: 3px;
}
.imitation-table-wrap .imitation-table-group .table-button-group {
  position: absolute;
  right: 15px;
  top: 50%;
  margin-top: -15px;
}
.imitation-table-list.imitation-table-icon {
  min-height: 34px;
  position: relative;
  padding-left: 44px!important;
}
.imitation-table-list.imitation-table-icon > span {
  display: block;
  position: absolute;
  width: 34px;
  height: 34px;
  left: 0;
  top: 50%;
  margin-top: -17px;
}
.imitation-table-list.imitation-table-icon > span.serial {
  border: 1px solid #ddd;
  border-radius: 50%;
  text-align: center;
  line-height: 34px;
}
.imitation-table-list.imitation-table-icon > span.icon {
  background: url("../images/imitation_table_icon.png") no-repeat;
}
.imitation-table-list.imitation-table-icon > span.icon.one {
  background-position: 0 0;
}
.imitation-table-list.imitation-table-icon > span.icon.two {
  background-position: 0 -34px;
}
.imitation-table-list.imitation-table-icon > span.icon.three {
  background-position: 0 -68px;
}
.imitation-table-list.imitation-table-icon > span.icon.four {
  background-position: 0 -170px;
}
.imitation-table-content > h4 {
  margin-top: 0;
  margin-bottom: 2px;
  font-size: 16px;
}
.imitation-table-content > p {
  margin-bottom: 2px;
}
.imitation-table-content > p:last-child {
  margin-bottom: 0;
}
.imitation-table-content > p > span {
  margin-right: 10px;
}
.imitation-table-content > p > span:last-child {
  margin-right: 0;
}
.imitation-table-content > h4,
.imitation-table-content p {
  word-break: break-all;
}
.imitation-table-content .tips {
  color: #A0A0A0;
  font-size: 12px;
  padding-left: 35px;
  display: none;
}
.imitation-table-content .tips.show-tips {
  display: block;
}
.imitation-table-content .tips > span {
  margin-left: -35px;
}
.imitation-table-content .tips .fa.fa-pencil {
  margin-right: 5px;
}
.imitation-table-content .tips > input {
  width: 90%;
  border: 0;
}
.imitation-table-content .tips > input:focus {
  outline: none;
  outline-offset: 0;
}
/*release*/
.release-wrap .release-icon-title {
  display: inline-block;
  position: absolute;
  left: 20px;
  top: 10px;
  width: 48px;
  height: 48px;
  background: url("../images/version.png") no-repeat;
  background-size: cover;
}
.release-wrap .release-title {
  background-color: #EBF1FF;
  padding: 20px 20px 20px 85px;
  position: relative;
}
.release-wrap .release-title > span {
  margin-right: 15px;
  font-size: 12px;
}
.release-wrap .release-title > span.ip-addr {
  font-weight: 700;
  font-size: 18px;
}
.release-wrap .release-title > span:last-child {
  margin-right: 0;
}
.release-wrap .release-title > p {
  margin-bottom: 0;
  font-size: 12px;
}
.release-wrap .release-content {
  background-color: #fff;
  padding-bottom: 20px;
}
.release-wrap .tabbable-custom .nav-tabs {
  background-color: #EBF1FF;
  padding-left: 90px;
}
.release-wrap .tabbable-custom .nav-tabs > li > a:focus,
.release-wrap .tabbable-custom .nav-tabs > li > a:hover {
  background-color: transparent;
}
.release-wrap .tabbable-custom .nav-tabs > li.active > a {
  background-color: #fff;
}
.release-wrap .release-icon {
  display: inline-block;
  width: 15px;
  height: 20px;
  margin-right: 5px;
  background: url("../images/button_icon_group.png") no-repeat;
  vertical-align: middle;
}
.release-wrap .release-icon.patch {
  background-position: -10px -211px;
}
.release-wrap .release-icon.version {
  background-position: -9px -178px;
}
.release-wrap .form-group .panel {
  margin-top: 10px;
  margin-bottom: 0;
}
.release-wrap .form-group .panel .panel-heading,
.release-wrap .form-group .panel .panel-body {
  padding: 2px 10px;
}
.virtual-machine-group {
  zoom: 1;
}
.virtual-machine-group:after {
  clear: both;
  content: '';
  display: table;
}
.virtual-machine-group .virtual-machine-list {
  float: left;
  border: 2px solid #598EEE;
  border-radius: 20px;
  width: 100px;
  background-color: #fff;
  color: #598EEE;
  text-align: center;
  margin-right: 20px;
  margin-bottom: 10px;
}
.virtual-machine-group .virtual-machine-list.active {
  color: #fff;
  background-color: #00bb00;
}
.virtual-machine-group.show-virtual-machine .virtual-machine-list {
  position: relative;
  padding-left: 80px;
  float: none;
  border: 0;
  border-bottom: 1px solid #ddd;
  border-radius: 0;
  width: auto;
  text-align: left;
}
.virtual-machine-group.show-virtual-machine .virtual-machine-list .machine-code {
  display: block;
  width: 75px;
  position: absolute;
  left: 0;
  top: 0;
  color: #333;
}
.release-model-wrap {
  padding-right: 20px;
  margin-top: 40px;
  margin-bottom: 20px;
}
.release-model-wrap .release-model-title {
  zoom: 1;
}
.release-model-wrap .release-model-title:after {
  clear: both;
  display: table;
  content: '';
}
.release-model-wrap .release-model-title .model-list {
  float: left;
  width: 50%;
  border: 1px solid #ddd;
  text-align: center;
  padding: 6px;
  color: #ccc;
}
.release-model-wrap .release-model-title .model-list.active {
  background-color: #f1f1f1;
}
.release-model-wrap .release-model-title .model-list:last-child {
  border-left-color: transparent;
}
.release-model-wrap .release-model-content {
  margin-top: 10px;
  border: 1px solid #ddd;
  background-color: #f6f6f6;
  width: 615px;
  height: 400px;
  overflow-y: auto;
  position: relative;
}
.release-model-wrap .release-model-content .model-bg {
  position: absolute;
  width: 100%;
  height: 100%;
  background: url("../images/model_bg.png") no-repeat center;
}
.release-model-wrap .release-model-content > h5 {
  color: #ddd;
  margin-top: 250px;
  text-align: center;
}
/*release detail*/
.release-detail-wrap .release-title {
  border-bottom: 2px solid #598EEE;
}
.release-detail-wrap .release-model-title .model-list {
  color: #598EEE;
}
.release-detail-wrap .release-model-title .model-list.active {
  background-color: #6B9CF8;
  color: #fff;
  position: relative;
}
.release-detail-wrap .release-model-title .model-list.active:before {
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  left: 50%;
  margin-left: -3px;
  bottom: -13px;
  border: 7px solid #6B9CF8;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
}
.release-detail-wrap .release-model-content {
  background-color: #fff;
}
.release-status-mark {
  position: relative;
}
.release-status-mark:before {
  position: absolute;
  content: '';
  display: block;
  right: 0;
  top: 0;
  width: 148px;
  height: 91px;
  background: url("../images/issue_ing.png") no-repeat;
  background-size: cover;
}
.release-status-mark.mark-fail:before {
  background: url("../images/issue_fail.png") no-repeat;
  background-size: cover;
}
.release-status-mark.mark-success:before {
  background: url("../images/issue_success.png") no-repeat;
  background-size: cover;
}
.release-flow-wrap {
  padding: 20px 0 0 20px;
}
.release-flow {
  padding-left: 28px;
  position: relative;
}
.release-flow:before {
  display: inline-block;
  width: 1px;
  height: 100%;
  content: '';
  background-color: #ddd;
  position: absolute;
  left: 12px;
  top: 37px;
}
.release-flow .flow-list {
  position: relative;
  margin-bottom: 20px;
}
.release-flow .flow-list > h5 {
  color: #333;
  font-weight: 700;
  margin: 2px 0;
}
.release-flow .flow-list > p {
  color: #A0A0A0;
  font-size: 12px;
  margin-bottom: 0;
}
.release-flow .flow-list .list-icon {
  display: inline-block;
  width: 26px;
  height: 20px;
  background: url("../images/button_icon_group.png") no-repeat;
  background-position: -5px -278px;
  position: absolute;
  left: -27px;
  top: -2px;
}
.release-flow .flow-list.flow-list-title .list-icon {
  background-position: -5px -245px;
}
.release-flow .flow-list.flow-list-title > h5 {
  color: #598EEE;
  width: 65px;
  position: relative;
  z-index: 1;
  background-color: #fff;
}
.release-flow .flow-list.flow-list-title:before {
  width: 100%;
  height: 1px;
  background-color: #ddd;
  content: '';
  position: absolute;
  top: 7px;
  z-index: 0;
}
.release-flow .flow-list.error .list-icon {
  background-position: -5px -313px;
}
.release-flow .flow-list.error > h5 {
  color: #E50112;
}
/*virtual manage*/
.virtual-manage-wrap .release-icon-title {
  top: 18px;
}
.virtual-manage-wrap .check-manage-diary {
  padding: 10px 20px;
  border-bottom: 1px solid #ddd;
}
.virtual-manage-wrap .virtual-manage-content {
  padding: 20px 0 0 20px;
}
.virtual-manage-content .accordion .accordion-title-icon {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url("../images/imitation_table_icon.png") no-repeat -9px -6px;
  margin-right: 5px;
  vertical-align: bottom;
}
.virtual-manage-content .accordion .panel-title > a {
  font-weight: 700;
  font-size: 12px;
}
.virtual-manage-content .accordion .panel-title > a > i {
  font-weight: normal;
  margin-left: 5px;
  font-style: normal;
}
.virtual-manage-content .accordion .panel {
  font-size: 12px;
}
.virtual-manage-content .accordion .panel .panel-heading:hover {
  background-color: #EBF1FF;
}
.virtual-manage-content .accordion .panel.open {
  border-color: #4A7DD9;
  border-width: 2px;
}
.virtual-manage-content .accordion .panel .panel-body {
  padding: 0;
}
.virtual-manage-content .machine-list {
  padding: 10px 0 0 40px;
}
.virtual-manage-content .machine-list .serial {
  width: 18px;
  height: 18px;
  border: 1px solid #ddd;
  border-radius: 50%;
  background-color: #fff;
  display: inline-block;
  text-align: center;
  line-height: 16px;
  margin-right: 5px;
}
.virtual-manage-content .machine-list .machine-title {
  border-bottom: 1px solid #ddd;
  padding: 0 20px 10px 0;
}
.virtual-manage-content .machine-list .machine-action {
  display: none;
  margin-top: 10px;
  padding-left: 25px;
}
.virtual-manage-content .machine-list .machine-action > span {
  cursor: pointer;
}
.virtual-manage-content .machine-list:hover {
  background-color: #EBF1FF;
}
.virtual-manage-content .machine-list:hover .machine-action {
  display: block;
}
