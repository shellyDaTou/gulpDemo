* {
  padding: 0;
  margin: 0;
}
html,
body {
  width: 100%;
  height: 100%;
  overflow: hidden;
  font-family: "microsoft yahei";
}
ul {
  list-style: none;
}
/*=========public style=========*/
/*buttons*/
button {
  outline: none;
}
.btn {
  color: #9d9d9d;
  cursor: pointer;
  padding: 0 10px;
  vertical-align: middle;
  border-radius: 50px;
  -webkit-border-radius: 50px;
  -moz-border-radius: 50px;
  -ms-border-radius: 50px;
  -o-border-radius: 50px;
}
.btn.btn-sm {
  height: 45px;
  min-width: 120px;
  font-size: 20px;
}
.btn.color-theme {
  background: #0A245F;
  border: solid 2px #17A4C7;
  color: #fff;
}
.btn.color-theme.active {
  background: #17A4C7;
}
.btn.color-danger {
  background: #480E33;
  border: solid 2px #E60012;
  color: #E60012;
}
.btn.color-danger.active {
  background: #E60012;
  color: #fff;
}
.btn.color-org {
  background: #503E3C;
  border: solid 2px #FF9824;
  color: #FF9824;
}
.btn.color-org.active {
  background: #FF9824;
  color: #fff;
  position: relative;
}
.btn.color-org.active:after {
  content: "";
  width: 29px;
  height: 28px;
  position: absolute;
  top: -24px;
  right: 10px;
  background: url("../images/btn_warning.png") no-repeat;
}
/*animation*/
@-webkit-keyframes myfirst {
  50% {
    background: #d99027;
  }
  100% {
    background: #ff5a00;
  }
}
@keyframes myfirst {
  from {
    background: #d99027;
  }
  to {
    background: #ff5a00;
  }
}
@-moz-keyframes myfirst {
  from {
    background: #d99027;
  }
  to {
    background: #ff5a00;
  }
}
@-webkit-keyframes myactive {
  50% {
    transform: scale3d(0, 0, 0);
    -webkit-transform: scale3d(0, 0, 0);
    -moz-transform: scale3d(0, 0, 0);
    -o-transform: scale3d(0, 0, 0);
    -ms-transform: scale3d(0, 0, 0);
  }
  100% {
    transform: scale3d(1, 1, 1);
    -webkit-transform: scale3d(1, 1, 1);
    -moz-transform: scale3d(1, 1, 1);
    -o-transform: scale3d(1, 1, 1);
    -ms-transform: scale3d(1, 1, 1);
  }
}
@keyframes myactive {
  from {
    transform: scale3d(0, 0, 0);
    -webkit-transform: scale3d(0, 0, 0);
    -moz-transform: scale3d(0, 0, 0);
    -o-transform: scale3d(0, 0, 0);
    -ms-transform: scale3d(0, 0, 0);
  }
  to {
    transform: scale3d(1, 1, 1);
    -webkit-transform: scale3d(1, 1, 1);
    -moz-transform: scale3d(1, 1, 1);
    -o-transform: scale3d(1, 1, 1);
    -ms-transform: scale3d(1, 1, 1);
  }
}
@-moz-keyframes myactive {
  from {
    transform: scale3d(0, 0, 0);
    -webkit-transform: scale3d(0, 0, 0);
    -moz-transform: scale3d(0, 0, 0);
    -o-transform: scale3d(0, 0, 0);
    -ms-transform: scale3d(0, 0, 0);
  }
  to {
    transform: scale3d(1, 1, 1);
    -webkit-transform: scale3d(1, 1, 1);
    -moz-transform: scale3d(1, 1, 1);
    -o-transform: scale3d(1, 1, 1);
    -ms-transform: scale3d(1, 1, 1);
  }
}
/*theme color*/
.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes bounce {
  from,
  20%,
  53%,
  80%,
  to {
    -webkit-animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  40%,
  43% {
    -webkit-animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    -webkit-transform: translate3d(0, -30px, 0);
    transform: translate3d(0, -30px, 0);
  }
  70% {
    -webkit-animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    -webkit-transform: translate3d(0, -15px, 0);
    transform: translate3d(0, -15px, 0);
  }
  90% {
    -webkit-transform: translate3d(0, -4px, 0);
    transform: translate3d(0, -4px, 0);
  }
}
@keyframes bounce {
  from,
  20%,
  53%,
  80%,
  to {
    -webkit-animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  40%,
  43% {
    -webkit-animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    -webkit-transform: translate3d(0, -30px, 0);
    transform: translate3d(0, -30px, 0);
  }
  70% {
    -webkit-animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06);
    -webkit-transform: translate3d(0, -15px, 0);
    transform: translate3d(0, -15px, 0);
  }
  90% {
    -webkit-transform: translate3d(0, -4px, 0);
    transform: translate3d(0, -4px, 0);
  }
}
.bounce {
  -webkit-animation-name: bounce;
  animation-name: bounce;
  -webkit-transform-origin: center bottom;
  transform-origin: center bottom;
}
@-webkit-keyframes flash {
  from,
  50%,
  to {
    opacity: 1;
  }
  25%,
  75% {
    opacity: 0;
  }
}
@keyframes flash {
  from,
  50%,
  to {
    opacity: 1;
  }
  25%,
  75% {
    opacity: 0;
  }
}
@-webkit-keyframes flash-slow {
  from,
  75%,
  to {
    opacity: 1;
  }
  25%,
  50% {
    opacity: 0;
  }
}
@keyframes flash-slow {
  from,
  75%,
  to {
    opacity: 1;
  }
  25%,
  50% {
    opacity: 0;
  }
}
@-webkit-keyframes flash-fast {
  from,
  25%,
  to {
    opacity: 1;
  }
  50%,
  75% {
    opacity: 0;
  }
}
@keyframes flash-fast {
  from,
  25%,
  to {
    opacity: 1;
  }
  50%,
  75% {
    opacity: 0;
  }
}
.flash {
  -webkit-animation-name: flash;
  animation-name: flash;
}
.flash-slow {
  -webkit-animation-name: flash-slow;
  animation-name: flash-slow;
}
.flash-fast {
  -webkit-animation-name: flash-fast;
  animation-name: flash-fast;
}
