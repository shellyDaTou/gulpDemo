.has-feedback .form-control-feedback.glyphicon-search {
  color: #598EEE;
}
.form-horizontal .form-group .control-label {
  font-weight: 700;
}
.input-group .input-group-btn .form-control {
  padding: 6px 2px;
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
}
.input-group .input-group-btn .form-control.input-xsmall {
  width: 85px!important;
}
.input-group .input-group-btn ~ .form-control {
  margin-left: -1px;
}
.form.form-horizontal .form-group {
  margin: 0;
}
.form.form-horizontal .form-group .group-btn,
.form.form-horizontal .form-group .group-btn > .form-control {
  display: inline-block;
}
.form.form-horizontal .form-group .form-control {
  width: 200px;
  height: 30px;
  margin-right: 20px;
  padding: 2px 12px;
}
.form.form-horizontal .group-wrap {
  width: auto;
  display: inline-block;
  padding: 5px 5px 5px 15px;
  border-radius: 2px;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  -ms-border-radius: 2px;
  -o-border-radius: 2px;
}
.form.form-horizontal .group-wrap .form-control,
.form.form-horizontal .group-wrap .btn {
  height: 28px;
}
.form.form-horizontal .group-wrap .form-control {
  border: 0;
  width: 300px;
