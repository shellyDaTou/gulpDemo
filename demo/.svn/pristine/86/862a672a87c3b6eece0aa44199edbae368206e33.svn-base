/*table*/
.table-scrollable {
  margin-top: 10px;
  width: 100%;
  overflow: auto;
}
.table-scrollable .table thead > tr > th {
  background-color: #E3E4E9;
  color: #9e9e9e;
}
.table-scrollable .table thead > tr > th,
.table-scrollable .table tbody > tr > td {
  text-align: center;
  border: 1px solid #c3c3c3!important;
  vertical-align: middle;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: break-all;
  max-width: 250px;
}
.table-scrollable .table tbody > tr > td > a {
  margin-right: 10px;
}
.table-scrollable .table tbody > tr > td > a:last-child {
  margin-right: 0;
}
.table-scrollable .table tbody > tr > td .form-control {
  height: 28px;
  padding: 0 12px;
}
/*accordion*/
.accordion .panel .panel-title {
  padding-right: 20px;
  position: relative;
}
.accordion .panel .panel-title .accordion-toggle.accordion-toggle-styled:before {
  position: absolute;
  right: 5px;
  top: 3px;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: 18px;
  content: "\f0d8";
  color: #4A7DD9;
}
.accordion .panel .panel-title .accordion-toggle.accordion-toggle-styled.collapsed:before {
  content: "\f0d7";
}
.panel-group .panel {
  border-radius: 0;
}
.panel-default > .panel-heading {
  background-color: #fff;
}
.panel-default > .panel-heading a:hover,
.panel-default > .panel-heading a:focus {
  text-decoration: none;
}
.panel-default > .panel-heading a:focus {
  outline: 0;
  outline-offset: 0;
}
/*modal*/
.modal-header {
  padding: 10px 15px;
  background-color: #4A7DD9;
  color: #fff;
}
.modal-header .close {
  color: #fff!important;
  opacity: 1;
}
.modal-header .modal-title {
  font-size: 14px;
}
.modal-footer {
  padding: 10px 15px;
}
/*loading*/
.loading-warp {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.45);
  z-index: 999;
}
.loading-warp > .lading-img {
  background-image: url("../images/loading.gif");
  background-size: 50px 50px;
  width: 50px;
  height: 50px;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
/*paging*/
.paging-wrap > li {
  float: left;
  margin-right: 5px;
}
.paging-wrap > li .btn {
  padding: 4px 6px;
}
.paging-wrap > li > span {
  margin-top: 4px;
  display: inline-block;
}
.paging-wrap > li .form-control {
  height: 30px;
}
.paging-wrap > li:last-child {
  margin-right: 0;
}
/*popover*/
.pos-popover {
  position: relative;
  display: inline-block;
}
.pos-popover .popover-wrap {
  position: absolute;
  background-color: #fff;
  border: solid 1px #ddd;
  padding: 15px;
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -webkit-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -ms-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  -o-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
  max-height: 390px;
  z-index: 99;
  display: none;
}
.pos-popover .popover-wrap:before,
.pos-popover .popover-wrap:after {
  content: "";
  position: absolute;
  border-right: 10px solid transparent;
  border-left: 10px solid transparent;
}
.pos-popover .popover-wrap.bottom-right:before,
.pos-popover .popover-wrap.bottom-left:before {
  border-bottom: 14px solid #b9b9b9;
  top: -14px;
}
.pos-popover .popover-wrap.bottom-right:after,
.pos-popover .popover-wrap.bottom-left:after {
  border-bottom: 14px solid #fff;
  top: -13px;
}
.pos-popover .popover-wrap.top-right:before,
.pos-popover .popover-wrap.top-left:before {
  border-top: 14px solid #b9b9b9;
  bottom: -14px;
}
.pos-popover .popover-wrap.top-right:after,
.pos-popover .popover-wrap.top-left:after {
  border-top: 14px solid #fff;
  bottom: -13px;
}
.pos-popover .popover-wrap.bottom-right {
  top: 30px;
  left: 0;
}
.pos-popover .popover-wrap.bottom-right:before,
.pos-popover .popover-wrap.bottom-right:after {
  left: 15px;
}
.pos-popover .popover-wrap.bottom-left {
  top: 30px;
  right: 0;
}
.pos-popover .popover-wrap.bottom-left:before,
.pos-popover .popover-wrap.bottom-left:after {
  right: 15px;
}
.pos-popover .popover-wrap.top-right {
  bottom: 30px;
  left: 0;
}
.pos-popover .popover-wrap.top-right:before,
.pos-popover .popover-wrap.top-right:after {
  left: 15px;
}
.pos-popover .popover-wrap.top-left {
  bottom: 30px;
  right: 0;
}
.pos-popover .popover-wrap.top-left:before,
.pos-popover .popover-wrap.top-left:after {
  right: 15px;
}
.pos-popover .popover-wrap.list-group {
  padding: 0;
  border: 0;
}
.pos-popover .popover-wrap.list-group .list-group-item {
  border-radius: 0;
}
.pos-popover .popover-wrap.list-group .list-group-item:hover,
.pos-popover .popover-wrap.list-group .list-group-item.active {
  font-weight: bold;
}
.pos-popover:hover .popover-wrap {
  display: inline-block;
}
/*时间控件*/
.input-group {
  /*按钮组*/
}
.input-group .input-icon.right {
  position: relative;
}
.input-group .input-icon.right .form-control {
  padding-right: 25px;
  border-radius: 4px;
}
.input-group .input-icon.right .fa {
  position: absolute;
  right: 6px;
  top: 8px;
  z-index: 99;
  color: #b3b3b3;
  cursor: pointer;
}
.input-group .input-icon.right:first-child .form-control {
  border-top-right-radius: 0!important;
  border-bottom-right-radius: 0!important;
}
.input-group .input-icon.right:last-child .form-control {
  border-top-left-radius: 0!important;
  border-bottom-left-radius: 0!important;
}
.input-group .input-group-btn .btn {
  padding: 6px 12px;
}
.input-group .input-group-btn .btn:first-child {
  margin-right: -2px;
}
.input-group.col-padding {
  padding-right: 15px;
  padding-left: 15px;
}
/*弹窗最大高度*/
.height-overflow-350 {
  max-height: 350px;
  overflow-y: auto;
}
/*checkbox-inline 宽度限制*/
.checkbox-inline.checkbox-wd-120 {
  width: 120px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: break-all;
}
.checkbox-inline.checkbox-wd-120:first-child {
  margin-left: 10px;
}
/*左边固定 右边自动*/
.pos-left-wrap {
  position: relative;
}
.pos-left-wrap .pos-left-content {
  position: absolute;
  left: 0;
}
/*冒泡提示*/
#toast-container {
  top: 40%;
  left: 40%;
}
#toast-container > div {
  opacity: 1!important;
}
/*开关按钮  样式重构*/
.bootstrap-switch {
  border-radius: 2px!important;
  height: 22px;
  border-color: #d2d2d2!important;
}
.bootstrap-switch .bootstrap-switch-handle-on,
.bootstrap-switch .bootstrap-switch-handle-off,
.bootstrap-switch .bootstrap-switch-label {
  line-height: 20px!important;
  padding: 0 3px!important;
  font-size: 12px!important;
  height: 20px;
}
.bootstrap-switch .bootstrap-switch-handle-on.bootstrap-switch-primary,
.bootstrap-switch .bootstrap-switch-handle-off.bootstrap-switch-primary,
.bootstrap-switch .bootstrap-switch-label.bootstrap-switch-primary {
  background: #5A8FED!important;
}
/*加载更多*/
.pos-more-line {
  position: relative;
  border-bottom: solid 1px #bcbcbc;
  margin: 20px 0;
}
.pos-more-line .more-text {
  position: absolute;
  text-align: center;
  top: -10px;
  padding: 0 10px;
  background-color: #e3e4e9;
  color: #598EEE;
}
/*旋转图标*/
@-webkit-keyframes swiper-preloader-spin {
  100% {
    -webkit-transform: rotate(360deg);
  }
}
@-moz-keyframes swiper-preloader-spin {
  100% {
    -webkit-transform: rotate(360deg);
  }
}
@-ms-keyframes swiper-preloader-spin {
  100% {
    -webkit-transform: rotate(360deg);
  }
}
@keyframes swiper-preloader-spin {
  100% {
    transform: rotate(360deg);
  }
}
.icon-rotate {
  width: auto!important;
  height: auto!important;
  display: inline-block;
  animation: swiper-preloader-spin 1s infinite linear;
  -moz-animation: swiper-preloader-spin 1s infinite linear;
  -webkit-animation: swiper-preloader-spin 1s infinite linear;
}
